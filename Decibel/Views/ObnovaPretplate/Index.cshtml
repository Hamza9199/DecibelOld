@model IEnumerable<Decibel.Models.ObnovaPretplate>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.KorisnikPretplata)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.datumObnove)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.iznosObnove)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.kreiranDatumVrijeme)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.KorisnikPretplata.ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.datumObnove)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.iznosObnove)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.kreiranDatumVrijeme)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
